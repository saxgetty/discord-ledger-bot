{
  "name": "doki-doki-wow-bot",
  "version": "1.0.0",
  "description": "Discord bot for Doki Doki Kawaii Club - WoW guild management including consumable reimbursements and BoE tracking",
  "main": "dist/index.js",
  "scripts": {
    "postinstall": "prisma generate",
    "migrate": "prisma migrate deploy",
    "dev": "ts-node-dev --respawn src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "register": "ts-node src/utils/registerCommands.ts",
    "seed:birthdays": "ts-node src/utils/seedBirthdays.ts"
  },
  "keywords": [
    "discord",
    "bot",
    "wow",
    "ledger",
    "guild"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "discord.js": "^14.16.3",
    "dotenv": "^17.2.4",
    "node-cron": "^4.2.1"
  },
  "devDependencies": {
    "@types/node": "^25.2.3",
    "prisma": "^5.22.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.7.2"
  }
}
